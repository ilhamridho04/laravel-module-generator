{
    "name": "ngodingskuyy/laravel-module-generator",
    "description": "Modular CRUD Generator for Laravel + Vue + Tailwind (shadcn-vue)",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "module",
        "generator",
        "crud",
        "vue",
        "tailwind",
        "shadcn-vue"
    ],
    "authors": [
        {
            "name": "Ilham Ridho Asysyifa'a",
            "email": "ilhamridho.ir@gmail.com",
            "homepage": "https://github.com/ilhamridho04",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "NgodingSkuyy\\LaravelModuleGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NgodingSkuyy\\LaravelModuleGenerator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "homepage": "https://github.com/ilhamridho04/laravel-module-generator",
    "support": {
        "issues": "https://github.com/ilhamridho04/laravel-module-generator/issues",
        "source": "https://github.com/ilhamridho04/laravel-module-generator"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.x-dev"
        },
        "laravel": {
            "providers": [
                "NgodingSkuyy\\LaravelModuleGenerator\\LaravelModuleGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}