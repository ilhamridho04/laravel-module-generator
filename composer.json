{
    "name": "ngodingskuyy/laravel-module-generator",
    "description": "Modular CRUD Generator for Laravel + Vue + Tailwind (shadcn-vue)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ilham Ridho Asysyifa'a",
            "email": "ilhamridho.ir@gmail.com",
            "homepage": "https://github.com/ilhamridho04",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10 || ^11",
        "spatie/laravel-permission": "^5.11"
    },
    "autoload": {
        "psr-4": {
            "NgodingSkuyy\\LaravelModuleGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NgodingSkuyy\\LaravelModuleGenerator\\LaravelModuleGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}